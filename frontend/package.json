{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rimraf ../public/{img,js,css,hot}* & rimraf ../public/*.{js,json,ico,txt} & vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint",
    "build_ux": "rm -rf ../public/{js,css,img,hot} && vue-cli-service build --no-clean",
    "build_win": "rimraf ../public/{img,js,css,hot}* & rimraf ../public/*.{js,json,ico,txt} & vue-cli-service build --no-clean"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "ajv-keywords": "^3.5.2",
    "axios": "^0.19.2",
    "bootstrap-sass": "^3.4.1",
    "core-js": "^3.6.5",
    "cross-env": "^6.0.3",
    "imagemin": "^6.0.0",
    "jquery": "^3.5.1",
    "laravel-echo": "^1.8.1",
    "laravel-mix": "^5.0.5",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "level": "^6.0.1",
    "lodash": "^4.17.19",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^5.0.1",
    "moment": "^2.27.0",
    "node-gyp-build": "^4.2.3",
    "node-gyp-install": "^2.2.0",
    "numeral": "^2.0.6",
    "socket.io-client": "^2.2.0",
    "uws": "^100.0.1",
    "vanilla-masker": "^1.2.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-goodshare": "^1.5.1",
    "vue-i18n": "^8.20.0",
    "vue-jstree": "^2.1.6",
    "vue-router": "^3.4.2",
    "vue-template-compiler": "^2.6.11",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.3.8",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "preload-webpack-plugin": "^2.3.0",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ]
    },
    "globals": {
      "Vue": "readonly"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
